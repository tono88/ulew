            <field name="field_type">boolean</field>
        </record>
        <record id="product_details_field_8" model="product.fields">
            <field name="field_name">uom_id</field>
            <field name="field_string">Unit of Measure</field>
            <field name="field_type">many2one</field>
        </record>
		<record id="product_details_field_9" model="product.fields">
            <field name="field_name">item_ids</field>
            <field name="field_string">Tarifas</field>
            <field name="field_type">one2many</field>
			<field name="field_x_tarifa">item_ids</field>
        </record>
    </data>
</odoo>