<odoo>
    <data noupdate="1">
        <record id="product_details_field_1" model="product.fields">
            <field name="field_name">barcode</field>
            <field name="field_string">Barcode</field>
            <field name="field_type">char</field>
        </record>
        <record id="product_details_field_2" model="product.fields">
            <field name="field_name">lst_price</field>
            <field name="field_string">Sale Price</field>
            <field name="field_type">float</field>
        </record>
        <record id="product_details_field_3" model="product.fields">
            <field name="field_name">standard_price</field>
            <field name="field_string">Cost Price</field>
            <field name="field_type">float</field>
        </record>
        <record id="product_details_field_4" model="product.fields">
            <field name="field_name">volume</field>
            <field name="field_string">Volume</field>
            <field name="field_type">float</field>
        </record>
        <record id="product_details_field_5" model="product.fields">
            <field name="field_name">weight</field>
            <field name="field_string">Weight</field>
            <field name="field_type">float</field>
        </record>
        <record id="product_details_field_6" model="product.fields">
            <field name="field_name">categ_id</field>
            <field name="field_string">Internal Category</field>
            <field name="field_type">many2one</field>
        </record>
        <record id="product_details_field_7" model="product.fields">
            <field name="field_name">to_weight</field>
